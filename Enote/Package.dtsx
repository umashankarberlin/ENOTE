				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Oct' then '10'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Nov' then '11'
				else
				'12'
				end
	+'.'+SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),8,4)
	ORDER BY ACC.ID_PERSON ASC
	;</property>
