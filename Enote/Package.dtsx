                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  ACC.ID_PERSON,cASE WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Jan' then '01'
	            WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Feb' then '02'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Mar' then '03'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Apr' then '04'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='May' then '05'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Jun' then '06'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Jul' then '07'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Aug' then '08'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Sep' then '09'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Oct' then '10'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Nov' then '11'
				else
				'12'
				end
	+'.'+SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),8,4) AS MONTH ,SUM([transaction_amount]) AS SUM_OF_TRANSACTION
	FROM TRANSACTIONS TR
	inner join account ACC ON ACC.ID_ACCOUNT = TR.ID_ACCOUNT
	inner join CUSTOMER_DET CUS ON CUS.ID_PERSON = ACC.ID_PERSON
	AND ACC.ID_PERSON IN('1234','345')
	AND TRANSACTION_DATE BETWEEN '2020-02-15'  AND '2020-06-06'
	GROUP BY ACC.ID_PERSON,
	cASE WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Jan' then '01'
	            WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Feb' then '02'
				WHEN SUBSTRING(CAST(TRANSACTION_DATE AS NVARCHAR),CHARINDEX(CAST(TRANSACTION_DATE AS NVARCHAR),'-',1)+1,3) ='Mar' then '03'
